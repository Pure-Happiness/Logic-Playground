<?xml version="1.0" encoding="utf-8"?>
<UserControl
    x:Class="Logic_Playground.TipET"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <TextBlock>
		<Run Text="ET ID idA idB" FontFamily="{StaticResource CascadiaCode}"/>
		<LineBreak/>
		<Run Text="where "/>
		<Run Text="idA" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text=" is the name of the theorem "/>
		<Run Text="a=b" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text=","/>
		<LineBreak/>
        <Run Text="and "/>
        <Run Text="idB" FontFamily="{StaticResource CascadiaCode}"/>
        <Run Text=" is the name of the theorem "/>
        <Run Text="b=c" FontFamily="{StaticResource CascadiaCode}"/>
        <Run Text=","/>
        <LineBreak/>
		<Run Text="produces a theorem "/>
		<Run Text="a=c" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text=" named "/>
		<Run Text="ID" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text="."/>
    </TextBlock>
</UserControl>
