<?xml version="1.0" encoding="utf-8"?>
<UserControl
    x:Class="Logic_Playground.TipExport"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <TextBlock>
        <Run Text="Export ID" FontFamily="{StaticResource CascadiaCode}"/>
        <LineBreak/>
        <Run Text="exports the theorem named "/>
        <Run Text="ID" FontFamily="{StaticResource CascadiaCode}"/>
        <Run Text=" to the parent scope."/>
        <LineBreak/>
        <Run x:Uid="MoreInformation"/>
    </TextBlock>
</UserControl>
