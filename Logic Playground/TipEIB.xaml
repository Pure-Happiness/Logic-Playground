<?xml version="1.0" encoding="utf-8"?>
<UserControl
    x:Class="Logic_Playground.TipEIB"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <TextBlock>
		<Run Text="EIB ID id" FontFamily="{StaticResource CascadiaCode}"/>
		<LineBreak/>
		<Run Text="where "/>
		<Run Text="id" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text=" is the name of the theorem "/>
        <Run Text="a" FontFamily="{StaticResource CascadiaCode}"/>
        <Run Text=","/>
        <LineBreak/>
		<Run Text="produces a theorem "/>
		<Run Text="a=true" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text=" named "/>
		<Run Text="ID" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text="."/>
    </TextBlock>
</UserControl>
