<?xml version="1.0" encoding="utf-8"?>
<Application
    x:Class="Logic_Playground.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
            </ResourceDictionary.MergedDictionaries>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Light">
                    <SolidColorBrush x:Key="ThemeGreenYellow" Color="#00FF00"/>
                </ResourceDictionary>
                <ResourceDictionary x:Key="Dark">
                    <SolidColorBrush x:Key="ThemeGreenYellow" Color="#FFFF00"/>
                </ResourceDictionary>
            </ResourceDictionary.ThemeDictionaries>
            <FontFamily x:Key="CascadiaCode">ms-appx:///Assets/CascadiaCode-Regular.ttf#Cascadia Code</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
