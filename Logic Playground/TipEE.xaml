<?xml version="1.0" encoding="utf-8"?>
<UserControl
    x:Class="Logic_Playground.TipEE"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <TextBlock>
		<Run Text="EE ID id f" FontFamily="{StaticResource CascadiaCode}"/>
		<LineBreak/>
		<Run Text="where "/>
		<Run Text="id" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text=" is the name of the theorem "/>
		<Run Text="a=b" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text=","/>
		<LineBreak/>
        <Run Text="and the parameter of "/>
        <Run Text="f" FontFamily="{StaticResource CascadiaCode}"/>
        <Run Text=" is of the same type of "/>
        <Run Text="a" FontFamily="{StaticResource CascadiaCode}"/>
        <Run Text=","/>
        <LineBreak/>
		<Run Text="produces a theorem "/>
		<Run Text="f#a=f#b" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text=" named "/>
		<Run Text="ID" FontFamily="{StaticResource CascadiaCode}"/>
		<Run Text="."/>
    </TextBlock>
</UserControl>
